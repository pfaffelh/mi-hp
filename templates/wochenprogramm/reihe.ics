BEGIN:VCALENDAR
VERSION:2.0
CALSCALE:GREGORIAN
PRODID:-//{{kurzname}}//DE
{% for event in events %}
BEGIN:VEVENT
UID:{{event["uid"]}}
SUMMARY:{{ event["title"] }}
LOCATION:{{ event["extendedProps"]["ort"]}}
DESCRIPTION:{{ event["extendedProps"]["description2"].replace('\n', '\\n') }}
{{ event["extendedProps"]["icsTime"]}}
END:VEVENT
{% endfor %}
END:VCALENDAR
