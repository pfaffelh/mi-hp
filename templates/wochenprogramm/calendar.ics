BEGIN:VCALENDAR
BEGIN:VTIMEZONE
TZID:Europe/Berlin
BEGIN:STANDARD
TZOFFSETFROM:+0200
TZOFFSETTO:+0100
TZNAME:CET
DTSTART:20241027T030000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=-1SU
END:STANDARD
BEGIN:DAYLIGHT
TZOFFSETFROM:+0100
TZOFFSETTO:+0200
TZNAME:CEST
DTSTART:20250329T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=-1SU
END:DAYLIGHT
END:VTIMEZONE
VERSION:2.0
CALSCALE:GREGORIAN
PRODID:-//{{name}}//DE
{% for event in events %}
BEGIN:VEVENT
UID:{{event["uid"]}}
SUMMARY:{{ event["title"] }}
LOCATION:{{ event["extendedProps"]["ort"]}}
DESCRIPTION:{{ event["extendedProps"]["description2"].replace('\n', '\\n') }}
{{ event["extendedProps"]["icsTime"]}}
END:VEVENT
{% endfor %}
END:VCALENDAR